{
  "manifest_version": 3,
  "name": "Malicious URL Detector",
  "version": "1.0",
  "description": "Detect and alert on malicious URLs.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "1": "icon.png"

    }
  }
}
